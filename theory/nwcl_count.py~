
import sys

def nwcl(filename) :

    f = open(filename, encoding="utf-8")

    linelist = f.readlines()

    count = 0
    for i in linelist :
        print(i)
        opt1 =  (i[0:2] != "--")
        opt2 =  i.strip()
        if (opt1 and opt2) :
            count += 1

    return count


if __name__ == "__main__" :

    src = "../imp/build/code/"
    filename = sys.argv[1]
    print(nwcl(src + filename))
